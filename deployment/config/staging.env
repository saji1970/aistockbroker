# AI Stock Trading System - Staging Environment Configuration

# Environment
ENVIRONMENT=staging
NODE_ENV=development

# Google Cloud Platform
GCP_PROJECT_ID=stockbroker-28983
GCP_REGION=us-central1
GCP_ZONE=us-central1-a

# API Keys (use environment variables in production)
GOOGLE_API_KEY=your_google_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
FINNHUB_API_KEY=your_finnhub_api_key_here
MARKETSTACK_API_KEY=your_marketstack_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///staging.db
REDIS_URL=redis://localhost:6379

# Application Configuration
DEBUG=true
LOG_LEVEL=DEBUG
API_RATE_LIMIT=1000
MAX_CONCURRENT_REQUESTS=100

# Trading Configuration
TRADING_MODE=shadow
INITIAL_CAPITAL=10000
MAX_POSITION_SIZE=1000
RISK_TOLERANCE=0.02
STOP_LOSS_PERCENTAGE=0.05
TAKE_PROFIT_PERCENTAGE=0.10

# Monitoring and Alerting
ENABLE_MONITORING=true
ENABLE_ALERTS=false
ALERT_EMAIL=admin@example.com
SLACK_WEBHOOK_URL=

# Security
SECRET_KEY=staging-secret-key-change-in-production
JWT_SECRET=staging-jwt-secret-change-in-production
CORS_ORIGINS=http://localhost:3000,https://staging.example.com

# Performance
CACHE_TTL=300
REQUEST_TIMEOUT=30
MAX_RETRIES=3
BACKOFF_FACTOR=2

# External Services
ALPHA_VANTAGE_BASE_URL=https://www.alphavantage.co/query
FINNHUB_BASE_URL=https://finnhub.io/api/v1
MARKETSTACK_BASE_URL=http://api.marketstack.com/v1

# Cloud Storage
STORAGE_BUCKET=stockbroker-28983-ai-stock-staging-data
STORAGE_REGION=us-central1

# Cloud Scheduler
SCHEDULER_TIMEZONE=America/New_York
TRADING_SCHEDULE=0 9 * * 1-5
DATA_UPDATE_SCHEDULE=0 */15 * * 1-5

# Health Check
HEALTH_CHECK_INTERVAL=60
HEALTH_CHECK_TIMEOUT=10
